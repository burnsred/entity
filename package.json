{
  "author": "BurnsRED",
  "license": "MIT",
  "type": "module",
  "name": "burnsred-entity-library",
  "private": true,
  "repository": "https://github.com/burnsred/burnsred-entity-library.git",
  "version": "0.6.2",
  "workspaces": [
    "packages/*"
  ],
  "engines": {
    "node": "16.16.0",
    "npm": "8.11.0"
  },
  "scripts": {
    "check": "npm run check:types",
    "check:types": "tsc",
    "clean": "lerna clean",
    "coveralls": "jest --config configs/jest.config.js --coverage --collectCoverageFrom=packages/**/*.{js,jsx} --collectCoverageFrom=!packages/**/*stories.{js,jsx} --maxWorkers=2 && cat ./coverage/lcov.info | coveralls",
    "deploy": "./scripts/deploy-ghpages.sh",
    "extract-messages": "extract-messages -l=en -o packages/dev-client/src/translation '**/**locales.js'",
    "fetch": "git fetch --prune",
    "git-view": "git log --all --graph --decorate --pretty=oneline --abbrev-commit",
    "lerna": "lerna",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:css": "stylelint '**/*.{js,jsx,ts,tsx}'",
    "lint:js": "eslint .",
    "packages-build": "lerna run --scope '@burnsred/*' build",
    "packages-destroy": "lerna run --scope '@burnsred/*' destroy",
    "packages-publish": "./scripts/publish.sh",
    "test": "jest --config configs/jest.config.js",
    "yarn": "yarn"
  },
  "dependencies": {
    "@burnsred/default": "0.6.2",
    "core-js": "^3.25.5",
    "lodash": "4.17.11",
    "prop-types": "15.7.2",
    "react": "16.8.4",
    "react-intl": "2.8.0"
  },
  "devDependencies": {
    "@babel/core": "7.19.3",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/plugin-proposal-export-default-from": "7.18.10",
    "@babel/plugin-transform-react-jsx": "7.19.0",
    "@babel/plugin-transform-runtime": "^7.19.1",
    "@babel/preset-env": "7.19.4",
    "@babel/preset-react": "7.18.6",
    "@babel/preset-typescript": "7.18.6",
    "@rollup/plugin-babel": "^6.0.0",
    "@rollup/plugin-commonjs": "^23.0.0",
    "@rollup/plugin-json": "^5.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-replace": "^5.0.0",
    "@types/jest": "29.1.2",
    "@typescript-eslint/eslint-plugin": "5.40.0",
    "babel-jest": "29.1.2",
    "babel-plugin-idx": "2.4.0",
    "babel-plugin-lodash": "3.3.4",
    "babel-plugin-react-intl-auto": "3.3.0",
    "babel-plugin-styled-components": "2.0.7",
    "babel-plugin-transform-react-remove-prop-types": "0.4.24",
    "coveralls": "3.0.3",
    "eslint": "8.25.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "27.1.1",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-promise": "6.0.1",
    "eslint-plugin-react": "7.31.10",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-unicorn": "44.0.2",
    "jest": "29.1.2",
    "jest-environment-jsdom": "^29.1.2",
    "jest-prop-type-error": "1.1.0",
    "jest-styled-components": "7.1.1",
    "jest-workspace-resolver": "^1.0.0",
    "lerna": "3.13.1",
    "react-test-renderer": "16.8.4",
    "rimraf": "2.6.3",
    "rollup": "3.1.0",
    "rollup-plugin-terser": "7.0.2",
    "stylelint": "14.13.0",
    "stylelint-config-recommended": "9.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.10.0",
    "typescript": "3.3.3333",
    "yarn": "1.13.0"
  }
}
